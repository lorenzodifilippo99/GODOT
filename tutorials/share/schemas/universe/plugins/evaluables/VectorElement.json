{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Vector Element",
    "description": "Scalar Evaluable from vector evaluable element",
    "type": "object",
    "required": [
        "index",
        "input"
    ],
    "additionalProperties": false,
    "properties": {
        "input": {
            "type": "string",
            "description": "Name of input vector evaluable"
        },
        "index": {
            "type": "integer",
            "description": "element index (zero for first element)"
        }
    }
}