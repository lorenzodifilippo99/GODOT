{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Parameters",
  "description": "Orbit Determination Parameters configuration",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "groups": {
      "description": "Groups of parameters.",
      "type": "array",
      "uniqueItems": true,
      "items": {
        "description": "Group of parameters.",
        "$ref": "definitions.json#/definitions/group"
      }
    },
    "free": {
      "description": "List of free parameters",
      "type": "array",
      "uniqueItems": true,
      "items": {
        "description": "Any existing parameter name, parameter name with wildcards (* or ?), or group of parameters",
        "type": "string"
      }
    },
    "consider": {
      "description": "List of consider parameters",
      "type": "array",
      "uniqueItems": true,
      "items": {
        "description": "Any existing parameter name, parameter name with wildcards (* or ?), or group of parameters",
        "type": "string"
      }
    }
  }
}