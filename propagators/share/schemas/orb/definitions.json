{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Definitions",
    "description": "Basic definitions for the Orbit Determination Problem",
    "definitions": {
        "group": {
            "description": "Group of objects (e.g. parameters)",
            "type": "object",
            "additionalProperties": false,
            "required": [
                "name",
                "data"
            ],
            "properties": {
                "name": {
                    "description": "Name of the group",
                    "type": "string"
                },
                "data": {
                    "description": "List of objects in the group.",
                    "type": "array",
                    "uniqueItems": true,
                    "items": {
                        "description": "Name of the object in the group.",
                        "type": "string"
                    }
                }
            }
        }
    }
}